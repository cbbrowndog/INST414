   "source": [
    "from sklearn.metrics import pairwise_distances_argmin_min\n",
    "\n",
    "# Get the actual coordinates used for clustering\n",
    "coords = df_clean[[\"LATITUDE\", \"LONGITUDE\"]].values\n",
    "\n",
    "# Use pairwise distances to find nearest real point to each centroid\n",
    "closest_indices, _ = pairwise_distances_argmin_min(kmeans.cluster_centers_, coords)\n",
    "\n",
    "# Extract representative lat/lon for each cluster\n",
    "for cluster_id, idx in enumerate(closest_indices):\n",
    "    lat, lon = coords[idx]\n",
    "    print(f\"Cluster {cluster_id} Representative Point: {lat:.6f}, {lon:.6f}\")\n"
   ]
  },
